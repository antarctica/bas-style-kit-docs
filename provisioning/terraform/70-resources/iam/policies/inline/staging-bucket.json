{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "MinimalContinuousDeploymentPermissions",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:GetObjectAcl",
                "s3:PutObjectAcl"
            ],
            "Resource": [
                "arn:aws:s3:::style-kit-testing.web.bas.ac.uk",
                "arn:aws:s3:::style-kit-testing.web.bas.ac.uk/*"
            ]
        }
    ]
}
